	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#008E00"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"keyword.control.import"
			],
			"settings": {
				"foreground": "#7D4726"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#DF0002"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#3A00DC"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#C800A4"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#275A5E"
			}
		},
		{
			"scope": [
				"variable.language",
				"variable.other"
			],
			"settings": {
				"foreground": "#C800A4"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#C800A4"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#C900A4"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#438288"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {}
		},
		{
			"scope": "entity.name.function",
			"settings": {}
		},
		{
			"scope": "variable.parameter",
			"settings": {}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#790EAD"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#450084"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#450084"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#450084"
			}
		},
		{
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#790EAD"
			}
		},
		{
			"scope": "support.other.variable",
			"settings": {
				"foreground": "#790EAD"
			}
		},
		{
			"scope": "invalid",
			"settings": {}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#CD3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
