                    "settings": {
                        "foreground": "#98c379"
                    }
                },
                {
                    "scope": "entity.other.inherited-class",
                    "settings": {
                        "foreground": "#a3a3a3",
                        "fontStyle": "italic",
                    }
                },
                {
                    "scope": "storage.type",
                    "settings": {
                        "foreground": "#56b6c2",
                    }
