                        "foreground": "#ec8b64",
                    }
                },

                // Variables
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#d19a66"
                    }
                },
                {
                    "scope": "variable.language",
                    "settings": {
                        "foreground": "#69b5d3",
                    }
                },
                {
                    "scope": "variable.other",
                    "settings": {
                        "foreground": "#ae7cd1",
                    }
                },
                {
                    "scope": "variable.other.constant",
                    "settings": {
                        "foreground": "#d8b34d",
                        "fontStyle": "italic",
                    }
                },

                // Constants.
                {
                    "scope": [
                        "constant",
                        "constant.numeric",
                        "support.constant"
                    ],
                    "settings": {
                        "foreground": "#d68b6e",
                    }
                },
                {
                    "scope": "constant.language",
                    "settings": {
                        "foreground": "#56b6c2",
                    }
                },
                {
                    "scope": [
                        "constant.character.escape"
                    ],
                    "settings": {
                        "foreground": "#6ec7d6",
                    }
                },

                // Keywords
                {
                    "scope": [
