                    }
                },

                // Typst
                {
                    "scope": "punctuation.definition.bracket.typst",
                    "settings": {
                        "foreground": "#6ec7d6"
                    }
                },
                {
                    "scope": "punctuation.separator.colon.typst",
                    "settings": {
                        "foreground": "#6ec7d6"
                    }
                },
            ]
        },
    },
